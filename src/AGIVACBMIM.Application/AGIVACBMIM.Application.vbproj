  <PropertyGroup>
    <RootNamespace>AGIVACBMIM.Application</RootNamespace>
    <TargetFramework>netstandard2.0</TargetFramework>
    <OptionStrict>On</OptionStrict>
    <ImportedNamespaces>AGIVACBMIM.Business=False,Spectre.Console=False,Microsoft.VisualBasic=True,System=True,System.Collections=True,System.Collections.Generic=True,System.Diagnostics=True,System.Linq=True,System.Xml.Linq=True,System.Threading.Tasks=True,AGIVACBMIM.Application=True</ImportedNamespaces>
